{
  "version": 2,
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/index.html"
    }
  ]
}
